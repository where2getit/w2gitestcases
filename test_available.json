{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "qa_tester"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "Where2getit!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "span.icon-cog"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.s-tab > p > strong"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//tbody[@class='channel-products']/tr[6]/td[1]/input"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//tbody[@class='channel-products']/tr[7]/td[1]/input"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "css selector",
        "value": "input.marginR-10"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "save-btn"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Back to Launchpad"
      }
    }
  ]
}
